{
  "include": ["**/*.tsx", "**/*.ts"],
  "exclude": ["node_modules", "dist", "release"],
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "lib": ["ES2015", "DOM", "WebWorker"],
    "module": "ESNext",
    "target": "ES6",
    "jsx": "preserve",
    "outDir": "dist",
    "moduleResolution": "node"
  }
}
